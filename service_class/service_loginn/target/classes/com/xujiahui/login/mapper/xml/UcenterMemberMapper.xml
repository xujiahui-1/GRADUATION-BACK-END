<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xujiahui.login.mapper.UcenterMemberMapper">

    <select id="getRegisterNumByDay"
            resultType="java.lang.Integer">
        SELECT count(*) FROM graduation.student_member
        where date(student_member.gmt_create)=#{day}
    </select>

</mapper>